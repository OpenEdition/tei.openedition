      </xs:sequence>
      <xs:attributeGroup ref="tei:att.global.attribute.xmlid"/>
      <xs:attributeGroup ref="tei:att.global.attribute.xmllang"/>
      <xs:attributeGroup ref="tei:att.typed.attribute.type"/>
    </xs:complexType>
  </xs:element>
